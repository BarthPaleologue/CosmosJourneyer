{
    "name": "Cosmos Journeyer API Documentation",
    "out": "doc",
    "theme": "typedoc-github-theme",
    "exclude": ["**/node_modules/**/*.*", "**/test/**/*.*"],
    "entryPoints": [
        "src/ts/frontend/cosmosJourneyer.ts",
        "src/ts/frontend/starmap/starMap.ts",
        "src/ts/frontend/starSystemView.ts"
    ],
    "visibilityFilters": {
        "protected": false,
        "private": false,
        "inherited": true,
        "external": false,
        "@alpha": false,
        "@beta": false
    },
    "plugin": ["typedoc-github-theme", "typedoc-plugin-missing-exports"]
}
