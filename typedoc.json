{
    "name": "Cosmos Journeyer API Documentation",
    "out": "doc",
    "plugin": ["typedoc-github-theme", "typedoc-plugin-missing-exports"],
    "theme": "typedoc-github-theme",

    "entryPointStrategy": "expand",
    "entryPoints": ["src/ts"],
    "exclude": ["**/node_modules/**/*.*", "**/test/**/*.*"],

    "includeVersion": true,
    "excludeInternal": true,
    "excludePrivate": true
}
